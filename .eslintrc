{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true
  },
  "rules": {
    "camelcase": [
      "error", {
        "properties": "always"
      }
    ],
    "indent": [
      2,
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "semi": [
      "error",
      "always"
    ],
    "comma-style": [
      "error",
      "last"
    ],
    "array-bracket-spacing": [
      "error",
      "always"
    ],
    "comma-spacing": [
      "error", {
        "before": false,
        "after": true
      }
    ],
    "key-spacing": [
      "error", {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "keyword-spacing": [
      "error", {
        "overrides": {
          "if": { "after": false },
          "for": { "after": false },
          "while": { "after": false },
          "catch": { "after": false }
        }
      }
    ],
    "object-curly-spacing": [
      "error",
      "never"
    ],
    "semi-spacing": [
      "error", {
        "before": false,
        "after": true
      }
    ],
    "space-in-parens": [
      "error",
      "never"
    ],
    "space-infix-ops": "error",
    "space-before-blocks": "error",
    "no-unused-vars": [
      "error", {
        "vars": "all",
        "args": "after-used"
      }
    ],
    "no-dupe-args": "error",
    "no-irregular-whitespace": "error",
    "no-mixed-spaces-and-tabs": [
      "error",
      "smart-tabs"
    ],
    "no-multi-spaces": "error",
    "no-spaced-func": "error",
    "no-trailing-spaces": "error",
    "no-whitespace-before-property": "error"
  }
}